<extension xmlns="http://ns.adobe.com/air/extension/24.0">
	<id>com.distriqt.MediaPlayer</id>
	<versionNumber>4.6.0</versionNumber>
	<platforms>

		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.mediaplayer.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.mediaplayer.MediaPlayerExtension</initializer>
				<finalizer>com.distriqt.extension.mediaplayer.MediaPlayerExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-ARM64">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.mediaplayer.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.mediaplayer.MediaPlayerExtension</initializer>
				<finalizer>com.distriqt.extension.mediaplayer.MediaPlayerExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.mediaplayer.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.mediaplayer.MediaPlayerExtension</initializer>
				<finalizer>com.distriqt.extension.mediaplayer.MediaPlayerExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x64">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.mediaplayer.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.mediaplayer.MediaPlayerExtension</initializer>
				<finalizer>com.distriqt.extension.mediaplayer.MediaPlayerExtension</finalizer>
			</applicationDeployment>
		</platform>

		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libMediaPlayer.a</nativeLibrary>
				<initializer>MediaPlayerExtInitializer</initializer>
				<finalizer>MediaPlayerExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-x86">
			<applicationDeployment>
				<nativeLibrary>libMediaPlayer.a</nativeLibrary>
				<initializer>MediaPlayerExtInitializer</initializer>
				<finalizer>MediaPlayerExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="appleTV-ARM">
			<applicationDeployment>
				<nativeLibrary>libMediaPlayer.a</nativeLibrary>
				<initializer>MediaPlayerExtInitializer</initializer>
				<finalizer>MediaPlayerExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>

		<platform name="default"> 
			<applicationDeployment /> 
		</platform>
		
	</platforms>
</extension>
